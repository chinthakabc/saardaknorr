<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE database SYSTEM
 "http://db.apache.org/torque/dtd/database_3_1.dtd">

<database
  name="inventorysystemdb"
  defaultIdMethod="native"
  package="inventorysystem.dao">
	<table name="user" javaName="User" description="User">
                <column
			name="user_id"
			javaName="User_ID"
			required="true"
			primaryKey="true"
			type="INTEGER"
			size="30"
                        autoIncrement="true"
			description="Username"/>
                 <column
			name="employeenumber"
			javaName="employeeNumber"
			required="true"
                        unique="true"
			primaryKey="false"
			type="VARCHAR"
			size="9"
			description="Employee_No"/>
                <column
			name="firstname"
			javaName="firstName"
			required="true"
			primaryKey="false"
			type="VARCHAR"
			size="30"
			description="First Name"/>
                <column
                        name="middlename"
                        javaName="middleName"
                        required="true"
                        primaryKey="false"
                        type="VARCHAR"
                        size="30"
                        description="Middle Name"
                        />
		<column
			name="lastname"
			javaName="lastName"
			required="true"
			primaryKey="false"
			type="VARCHAR"
			size="30"
			description="Last Name"/>
                <column
			name="username"
			javaName="userName"
			required="true"
			unique="true"
			primaryKey="false"
			type="VARCHAR"
			size="30"
			description="UserName"/>
		<column
			name="password"
			javaName="password"
			required="true"
			primaryKey="false"
			type="VARCHAR"
			size="10"
			description="Password"/>
		<column
			name="contactnumber"
			javaName="contactNumber"
			required="true"
			primaryKey="true"
			type="VARCHAR"
			size="12"
			description="User_Contact_No"/>
                <column
			name="division"
			javaName="division"
			required="true"
			primaryKey="false"
			type="VARCHAR"
			size="80"
			description="Division"/>
                <column
			name="type"
			javaName="type"
			required="true"
			type="VARCHAR"
                        size="5"
			description="Type"/>
		<column
			name="status"
			javaName="Status"
			required="true"
			type="BOOLEANINT"
			description="Status"/>
	</table>
       
        <table name="supply" javaName="Supply" description="Supply">
                <column
			name="supply_id"
			javaName="Supply_ID"
			required="true"
			primaryKey="true"
			type="INTEGER"
			size="30"
                        autoIncrement="true"
			description="Supply_ID"/>
                 <column
			name="type"
			javaName="Type"
			required="true"
			primaryKey="false"
			type="VARCHAR"
			size="30"
			description="Type"/>
                 <column
			name="name"
			javaName="Name"
			required="true"
			primaryKey="false"
			type="VARCHAR"
			size="50"
			description="Name"/>
                 <column
			name="quantity"
			javaName="Quantity"
			required="true"
			primaryKey="false"
			type="INTEGER"
			size="20"
			description="Quantity"/>
                   <column
			name="source"
			javaName="Source"
			required="false"
			primaryKey="false"
			type="VARCHAR"
			size="30"
			description="Source"/>
                   <column
			name="location"
			javaName="Location"
			required="false"
			primaryKey="false"
			type="VARCHAR"
			size="80"
			description="Location"/>
	</table>
        <table name="manages" javaName="Manages" description="UserManagesSupply">
                <column
			name="user_id"
			javaName="User_ID"
			required="true"
			primaryKey="true"
			type="INTEGER"
			size="30"
			description="User_ID"/>
                 <column
			name="supply_id"
			javaName="Supply_ID"
			required="true"
			primaryKey="true"
			type="INTEGER"
			size="30"
			description="Supply_ID"/>
                 <foreign-key foreignTable="user">
			<reference local="user_id" foreign="user_id"/>
		</foreign-key>
                <foreign-key foreignTable="supply">
			<reference local="supply_id" foreign="supply_id"/>
		</foreign-key>
	</table>
        <table name="orf" javaName="ORF" description="ORF">
                <column
			name="orf_id"
			javaName="ORF_ID"
			required="true"
			primaryKey="true"
			type="INTEGER"
			size="30"
                        autoIncrement="true"
			description="ORF_ID"/>

                  <column
			name="status"
			javaName="Status"
			required="true"
			type="BOOLEANINT"
			description="Status"/>
                   <column
			name="user_id"
			javaName="User_ID"
			required="true"
			primaryKey="true"
			type="INTEGER"
			size="30"
			description="User_ID"/>
                   <column
			name="issue_date"
			javaName="Issue_Date"
			required="true"
			type="VARCHAR"
			size="30"
			description="Issue_Date"/>
                   <foreign-key foreignTable="user">
			<reference local="user_id" foreign="user_id"/>
		   </foreign-key>
	</table>
        <table name="orf_supply" javaName="ORF_Supply" description="ORF_Supply">
                <column
			name="orf_id"
			javaName="ORF_ID"
			required="true"
			primaryKey="true"
			type="INTEGER"
			size="30"
			description="ORF_ID"/>
                   <column
			name="user_id"
			javaName="User_ID"
			required="true"
			primaryKey="true"
			type="INTEGER"
			size="30"
			description="User_ID"/>
                     <column
			name="supply_type"
			javaName="Supply_Type"
			required="true"
			primaryKey="false"
			type="VARCHAR"
			size="30"
			description="Supply_Type"/>
                    <column
			name="supply_name"
			javaName="supply_name"
			required="true"
			primaryKey="false"
			type="VARCHAR"
			size="50"
			description="supply_name"/>
                    <column
			name="quantity"
			javaName="Quantity"
			required="true"
			primaryKey="false"
			type="VARCHAR"
			size="20"
			description="Quantity"/>
                  <column
			name="price"
			javaName="Price"
			required="true"
			primaryKey="false"
			type="VARCHAR"
			size="20"
			description="Price"/>
                   <foreign-key foreignTable="user">
			<reference local="user_id" foreign="user_id"/>
		   </foreign-key>
                    <foreign-key foreignTable="orf">
			<reference local="orf_id" foreign="orf_id"/>
		   </foreign-key>
	</table>
</database>
